{
	"name": "akore",
	"version": "1.8.0",
	"description": "BDFD based transpiler",
	"main": "dist/index.js",
	"files": ["./dist/**"],
	"scripts": {
		"generate-exports": "node ./scripts/generate_exports.js",
		"build": "node ./scripts/generate_exports.js && tsc && biome check --apply src/",
		"format": "biome format --apply src/",
		"check": "biome check --apply src/",
		"lint": "biome lint --apply src/"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Pavez7274/akore.git"
	},
	"author": "Pavez7274",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Pavez7274/akore/issues"
	},
	"homepage": "https://github.com/Pavez7274/akore#readme",
	"devDependencies": {
		"@biomejs/biome": "1.7.3",
		"@jest/globals": "29.7.0",
		"jest": "29.7.0",
		"ts-jest": "29.1.3",
		"ts-node": "10.9.2",
		"typescript": "5.4.5"
	},
	"exports": {
		".": "./dist/index.js",
		"./javascript": "./dist/common/javascript/index.js",
		"./common": "./dist/common/index.js"
	},
	"imports": {
		"#*": ["./dist/*.js"],
		"#structures": ["./dist/structures/index.js"],
		"#common": ["./dist/common/index.js"]
	},
	"type": "commonjs"
}
